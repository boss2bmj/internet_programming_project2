<h1>Listing Car Details</h1>
<p>/car_detail/adminpage</p>
<a href=""><%= link_to "Sign Out", destroy_admin_session_path, method: :delete %><br></a>
<table>
  <thead>
    <tr>
      <th>Car license</th>
      <th>Description</th>
      <th>Enter date</th>
      <th>Car category</th>
      <th>Car subcategory</th>
      <th>Car status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_details.each do |car_detail| %>
      <tr>
        <td><%= car_detail.car_license %></td>
        <td><%= car_detail.description %></td>
        <td><%= car_detail.enter_date %></td>
        <td><%= car_detail.car_category %></td>
        <td><%= car_detail.car_subcategory %></td>
        <td><%= car_detail.car_status %></td>
        <td><%= link_to 'Show', car_detail %></td>
        <td><%= link_to 'Edit', edit_car_detail_path(car_detail) %></td>
        <td><%= link_to 'Destroy', car_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car detail', new_car_detail_path %>
